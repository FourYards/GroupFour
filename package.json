{
  "name": "yard-work-marketplace",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "test": "npm run test:client",
    "test:client": "vitest run --environment jsdom --root ./client/test",
    "test:client:watch": "vitest watch --environment jsdom --root ./client/test",
    "dev": "nodemon",
    "migrate": "sequelize-cli db:migrate",
    "seed": "sequelize-cli db:seed:all",
    "debug": "nodemon --inspect ./bin/www",
    "debug:nowatch": "node --inspect ./bin/www",
    "repl": "node ./scripts/repl.js",
    "lint": "eslint . --ext .js,.cjs,.mjs,.vue --fix --ignore-path .gitignore",
    "formatcheck": "prettier --check .",
    "format": "prettier --write .",
    "prepare": "node -e \"try { require('husky').install() } catch (e) {if (e.code !== 'MODULE_NOT_FOUND') throw e}\" && npx --yes npm-merge-driver@latest install"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "cookie-parser": "~1.4.4",
    "express": "~4.16.1",
    "handlebars-helpers": "^0.9.8",
    "hbs": "^4.2.0",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "sequelize": "^6.15.0",
    "sqlite3": "npm:@vscode/sqlite3@^5.0.7",
    "vue": "^3.2.31"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.0",
    "@vitejs/plugin-vue": "^2.2.4",
    "@vitejs/plugin-vue-jsx": "^1.3.8",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/test-utils": "^2.0.0-rc.17",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-vue": "^8.5.0",
    "husky": "^7.0.4",
    "jsdom": "^19.0.0",
    "lint-staged": "^12.3.4",
    "nodemon": "^2.0.15",
    "prettier": "2.5.1",
    "sequelize-cli": "^6.4.1",
    "vite": "^2.8.6",
    "vitest": "^0.6.0"
  },
  "nodemonConfig": {
    "ext": "js,hbs,json",
    "ignore": [
      "*.test.js",
      "client/*"
    ]
  },
  "lint-staged": {
    "*.{js,cjs,mjs,vue}": "eslint --cache --fix",
    "*.{js,cjs,mjs,hbs,css,json,md,prettierrc}": "prettier --write"
  }
}
