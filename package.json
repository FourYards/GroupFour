{
  "name": "yard-work-marketplace",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "test": "npm run test:client && npm run test:server",
    "test:client": "vitest run --environment jsdom --root ./client/test",
    "test:client:watch": "vitest watch --environment jsdom --root ./client/test",
    "test:server": "jest --roots ./server/test",
    "dev": "nodemon",
    "migrate": "sequelize-cli db:migrate",
    "migrate:undo": "sequelize-cli db:migrate:undo:all",
    "migrate:undo:specific": "sequelize-cli db:migrate:undo --name",
    "migrate:reset": "npm run migrate:undo && npm run migrate",
    "seed": "sequelize-cli db:seed:all",
    "seed:specific": "sequelize-cli db:seed --seed",
    "seed:undo": "sequelize-cli db:seed:undo:all",
    "seed:undo:specific": "sequelize-cli db:seed:undo --seed",
    "seed:reseed": "npm run seed:undo && npm run seed",
    "debug": "nodemon --inspect ./bin/www",
    "debug:nowatch": "node --inspect ./bin/www",
    "repl": "node ./scripts/repl.js",
    "lint": "eslint . --ext .js,.cjs,.mjs,.vue --fix --ignore-path .gitignore",
    "formatcheck": "prettier --check .",
    "format": "prettier --write .",
    "prepare": "node -e \"try { require('husky').install() } catch (e) {if (e.code !== 'MODULE_NOT_FOUND') throw e}\" && npx --yes npm-merge-driver@latest install"
  },
  "dependencies": {
    "@vue/composition-api": "^1.4.9",
    "argon2": "^0.28.5",
    "bootstrap-vue": "2.21.2",
    "chalk": "^4.1.2",
    "connect-session-sequelize": "^7.1.2",
    "cookie-parser": "~1.4.4",
    "csurf": "^1.11.0",
    "dotenv": "^16.0.0",
    "express": "~4.16.1",
    "express-session": "^1.17.2",
    "handlebars-helpers": "^0.9.8",
    "hbs": "^4.2.0",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "passport": "^0.5.2",
    "passport-local": "^1.0.0",
    "sequelize": "^6.15.0",
    "sqlite3": "npm:@vscode/sqlite3@^5.0.7",
    "vue": "^2.6.14",
    "vue-fragment": "^1.5.2"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.0",
    "@types/jest": "^27.4.1",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/runtime-dom": "^3.2.31",
    "@vue/test-utils": "^1.3.0",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-vue": "^8.5.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "jsdom": "^19.0.0",
    "lint-staged": "^12.3.4",
    "nodemon": "^2.0.15",
    "prettier": "2.5.1",
    "sequelize-cli": "^6.4.1",
    "unplugin-vue2-script-setup": "^0.10.0",
    "vite": "^2.8.6",
    "vite-plugin-full-reload": "^1.0.0",
    "vite-plugin-vue2": "^1.9.3",
    "vitest": "^0.6.0",
    "vue-template-compiler": "^2.6.14"
  },
  "overrides": {
    "node-gyp": "^8.4.0"
  },
  "nodemonConfig": {
    "ext": "js,hbs,json",
    "ignore": [
      "*.test.js",
      "client/*"
    ]
  },
  "lint-staged": {
    "*.{js,cjs,mjs,vue}": "eslint --cache --fix",
    "*.{js,cjs,mjs,hbs,css,json,md,prettierrc}": "prettier --write"
  }
}
